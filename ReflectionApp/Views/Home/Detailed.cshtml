@{
    ViewData["Title"] = "Assembly Information";
}



<h1>@ViewData["Title"]</h1>
<input type="button" value="Browse New Assembly" onclick='window.location.href="@Url.Action("Index","Home")";' />
<hr />
<p><strong>Full Assembly Name: </strong>@ViewBag.AssemblyName</p>

<div class="row">
    <div class="col-md-12">
        <table id="assemblyTable" class="table table-striped table-bordered" style="width:100%">
            <thead>
                <tr><th>Types</th><th>No. of Methods</th><th>Methods</th></tr>
            </thead>
            <tbody>
                @foreach (var type in ViewBag.Types)
                {
                    <tr>
                        <td>@type.type</td>
                        <td>@type.no_of_methods</td>
                        <td>@type.method</td>
                    </tr>

                }
            </tbody>

        </table>
    </div>
</div>
