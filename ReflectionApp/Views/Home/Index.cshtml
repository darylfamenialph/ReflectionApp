
<div class="text-center">
    <h3 class="display-4">Upload an Assembly(DLL)</h3><br />
    <br />
    <form method="post" enctype="multipart/form-data" asp-controller="FileUpload" asp-action="Index">
        <div class="form-group">
            <div class="col-md-10">
                <p>Upload single file:</p>
                <input type="file" name="file" />
            </div>
        </div>
        <br />
        <div class="form-group">
            <div class="col-md-10">
                <input class="btn btn-primary" type="submit" value="Reflect this Assembly" />
            </div>
        </div>
    </form>
</div>
